{
  "private": true,
  "repository": "https://github.com/tannerlinsley/react-virtual.git",
  "scripts": {
    "i": "yarn install --ignore-engines",
    "installAll": "lerna exec 'yarn --force'",
    "linkAll": "lerna exec 'yarn link' && lerna exec 'yarn link react-location'",
    "unlinkAll": "lerna exec yarn unlink",
    "test": "lerna run test",
    "build": "lerna run build",
    "build:ci": "cd packages/react-location && yarn build && cd ../.. && yarn linkAll && lerna run build",
    "watch": "lerna run watch --parallel",
    "prettier": "lerna run prettier",
    "start": "yarn watch",
    "format": "lerna run format",
    "doctoc": "lerna run doctoc",
    "cipublish": "ts-node scripts/publish.ts"
  },
  "workspaces": [
    "./packages/react-virtual"
  ],
  "dependencies": {
